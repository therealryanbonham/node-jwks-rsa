{
  "name": "jwks-rsa",
  "version": "1.3.0",
  "description": "Library to retrieve RSA public keys from a JWKS endpoint",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "dependencies": {
    "@types/express-jwt": "0.0.40",
    "debug": "~4.1.1",
    "limiter": "~1.1.3",
    "lru-memoizer": "^1.12.0",
    "ms": "~2.1.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.2.0",
    "eslint": "^5.12.0",
    "eslint-plugin-babel": "^5.3.0",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.4.0",
    "koa": "^2.6.2",
    "koa-jwt": "^3.5.1",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "rimraf": "^2.6.3",
    "supertest": "^3.3.0"
  },
  "scripts": {
    "clean": "rimraf lib/",
    "compile": "babel -d lib/ src/",
